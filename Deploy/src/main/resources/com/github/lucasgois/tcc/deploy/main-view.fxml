<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane hgap="5.0" prefHeight="400.0" prefWidth="600.0" vgap="5.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.lucasgois.tcc.deploy.MainController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="0.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="0.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="0.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0"/>
        <RowConstraints minHeight="10.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0"/>
    </rowConstraints>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <TableView fx:id="tbArquivos" prefHeight="0.0" prefWidth="0.0" GridPane.columnSpan="3" GridPane.rowIndex="2">
        <columns>
            <TableColumn fx:id="treeTableColumn1" prefWidth="100.0" text="Hash"/>
            <TableColumn fx:id="treeTableColumn2" prefWidth="200.0" text="File"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
    <Button onAction="#onDeploy" prefWidth="999.0" text="Deploy" GridPane.columnIndex="2" GridPane.rowIndex="3"/>
    <Button onAction="#onLoad" prefWidth="999.0" text="Load" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
    <Button onAction="#onExplorer" prefWidth="999.0" text="Explorer" GridPane.rowIndex="3"/>
    <Label text="Environment:" GridPane.columnIndex="2"/>
    <Label text="Module:" GridPane.columnIndex="1"/>
    <Label text="Version:"/>
    <ComboBox fx:id="cbEnvironment" prefWidth="9999.0" GridPane.columnIndex="2" GridPane.hgrow="SOMETIMES" GridPane.rowIndex="1"/>
    <ComboBox fx:id="cbModule" prefWidth="9999.0" GridPane.columnIndex="1" GridPane.hgrow="SOMETIMES" GridPane.rowIndex="1"/>
    <TextField fx:id="tfVersion" GridPane.rowIndex="1"/>
</GridPane>
